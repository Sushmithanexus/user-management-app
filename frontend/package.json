{
  "name": "user-management-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test --project=chromium --project=mobile-chrome --project=mobile-safari",
    "test:e2e:ui": "playwright test --ui --project=chromium --project=mobile-chrome --project=mobile-safari",
    "test:e2e:headed": "playwright test --headed --project=chromium --project=mobile-chrome --project=mobile-safari",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:isolated": "playwright test --config=playwright.isolated.config.js",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:mobile-chrome": "playwright test --project=mobile-chrome",
    "test:e2e:mobile-safari": "playwright test --project=mobile-safari",
    "test:e2e:tablet": "playwright test --project=tablet",
    "test:e2e:desktop": "playwright test --project=chromium",
